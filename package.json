{
  "name": "hello-nest-service",
  "version": "0.0.1",
  "description": "",
  "author": "HqO",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "db:wait": "wait-for-mysql --host ${DB_HOST} --port ${DB_PORT} --username ${DB_USER} --password ${DB_PASSWORD} --total-timeout 300000",
    "devspace:clean": "rm -rf .devspace",
    "devspace:deploy:ffm": "./scripts/devspace-deploy.sh -c development-ffm -a hqo",
    "devspace:deploy": "./scripts/devspace-deploy.sh -c development -a hqo",
    "devspace:install": "brew install devspace; devspace add plugin https://github.com/loft-sh/loft-devspace-plugin",
    "devspace:login": "devspace login https://loft.hqo.dev/",
    "devspace:restart:ffm": "./scripts/devspace-restart.sh -c development-ffm -a hqo",
    "devspace:restart:local": "yarn devspace:stop:local && yarn devspace:start:local",
    "devspace:restart": "./scripts/devspace-restart.sh -c development -a hqo",
    "devspace:start:ffm": "./scripts/devspace-start.sh -c development-ffm -a hqo",
    "devspace:start:local": "devspace dev -p local -n development --kube-context docker-desktop --no-warn",
    "devspace:start": "./scripts/devspace-start.sh -c development -a hqo",
    "devspace:stop:ffm": "./scripts/devspace-stop.sh -c development-ffm",
    "devspace:stop:local": "devspace purge -n development --kube-context docker-desktop --no-warn",
    "devspace:stop": "./scripts/devspace-stop.sh -c development",
    "devspace:update": "devspace upgrade && devspace update plugin loft",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "genenv:local": "cp ./development.env .env",
    "init-repo": "./scripts/init-repo.sh $1",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "redis:wait": "wait-for-redis --port ${REDIS_PORT} --quiet",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:local": "./scripts/start-local.sh",
    "start:prod": "node dist/main",
    "start": "nest start",
    "test:cov:ci": "yarn test --coverage --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:ci": "ENV_FILE=.env.test.ci jest --config test/jest-e2e.json --coverage --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:redis:wait:ci": "dotenv -e .env.test.ci -- yarn redis:wait",
    "test:db:wait:ci": "dotenv -e .env.test.ci -- yarn db:wait",
    "test:db:wait": "dotenv -e .env.test -- yarn db:wait",
    "test:watch": "jest --watch",
    "test": "ENV_FILE=.env.test NODE_ENV=testing jest",
    "types:check": "tsc --noEmit",
    "knex": "ts-node node_modules/knex/bin/cli.js --knexfile knexfile.ts",
    "migrate:make": "yarn knex migrate:make -x ts",
    "migrate:rollback": "yarn knex migrate:rollback",
    "migrate": "yarn knex migrate:latest",
    "seed:make": "./scripts/gen_seed.sh",
    "seed": "yarn run knex seed:run",
    "test:db:migrate:ci": "ENV_FILE=.env.test.ci yarn migrate",
    "test:db:migrate": "ENV_FILE=.env.test yarn migrate",
    "test:db:prepare:ci": "yarn test:db:wait:ci && yarn test:db:migrate:ci && yarn test:db:seed:ci",
    "test:db:seed:ci": "ENV_FILE=.env.test.ci yarn seed",
    "test:db:seed": "ENV_FILE=.env.test yarn seed"
  },
  "dependencies": {
    "@hqo/nestjs-winston-logger": "1.2.3",
    "@hqo/shared-modules": "0.0.4",
    "@hqo/users-service-common": "0.0.22",
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.17",
    "@nestjs/swagger": "4.8.0",
    "@nestjs/typeorm": "7.1.5",
    "@sentry/node": "6.4.1",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "config": "3.3.6",
    "dd-trace": "0.33.2",
    "dotenv": "9.0.2",
    "dotenv-cli": "4.0.0",
    "envkey": "1.3.1",
    "hot-shots": "7.4.2",
    "ioredis": "4.27.5",
    "knex": "0.95.6",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "mysql2": "2.2.5",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "retry-axios": "2.4.0",
    "rimraf": "3.0.2",
    "rxjs": "7.1.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.32",
    "uuid": "8.3.2",
    "wait-for-mysql": "1.0.0",
    "wait-for-redis": "1.0.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.17",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.1",
    "@types/passport-jwt": "3.0.5",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "depcheck": "1.4.1",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "26.6.3",
    "node-mocks-http": "1.10.1",
    "prettier": "2.3.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "9.2.3",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  },
  "resolutions": {
    "lodash": "4.17.21",
    "wait-for-mysql/mysql": "2.18.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
